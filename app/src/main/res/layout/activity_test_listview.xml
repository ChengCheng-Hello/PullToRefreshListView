<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.baijiahulian.common.listview.AbsListView
        android:id="@+id/test_listview_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:gsx_list_layout_header="@layout/layout_listview_header"
        app:gsx_list_layout_empty="@layout/layout_listview_empty"
        app:gsx_list_layout_error="@layout/layout_listview_error"
        app:gsx_list_layout_moreProgress="@layout/layout_listview_loadmore"
        app:gsx_list_layout_progress="@layout/layout_listview_progress"
        app:gsx_list_recyclerClipToPadding="false"
        app:gsx_list_recyclerMaxLength="false"
        app:gsx_list_recyclerScrollbarStyle="outsideOverlay" />



</LinearLayout>
